let k = [
(10, (2, 5, 10)),
(100, (5, 10, 25)),
(1000, (5, 25, 125)), 
(10000, (4, 40, 400)) 
]



let t = clock()
let sum = 0

for z in 0..10 {
for [n, w] in k {
let l = [0..n]
for s in w {
l.sort
for i in Range(0, n, s) {
let p = l[i..i+s]
p.reverse
l[i..i+s] = p
sum += l[-1] + l[0]
}
}
}
}


t = clock() -t
print('Sum: '+sum)
print('Elapsed: '+t)