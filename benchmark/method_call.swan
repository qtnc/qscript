class Toggle {
  constructor (startState as Bool) {
    _state = startState
_ac = 0
  }

  value () as Bool { _state }
  activate as Toggle {
    _state = !_state
    return this
  }

}

class NthToggle is Toggle {
  constructor (startState as Bool, maxCounter as Num) {
    super.constructor(startState)
    _countMax = maxCounter
    _count = 0
  }

  activate as NthToggle {
    _count += 1
    if (_count >= _countMax) {
      super.activate
      _count = 0
    }
    return this
  }
}

